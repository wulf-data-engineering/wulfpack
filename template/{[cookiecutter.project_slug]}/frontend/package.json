{
	"name": "tool-set-project-frontend",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && node scripts/inject-splash-into-fallback.mjs",
		"preview": "vite preview",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"proto:gen": "echo \"Not implemented yet\""
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.0",
		"@eslint/js": "^9.37.0",
		"@internationalized/date": "^3.10.0",
		"@lucide/svelte": "^0.545.0",
		"@playwright/test": "^1.56.0",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.46.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.18",
		"@tailwindcss/vite": "^4.1.13",
		"@types/node": "^24.7.0",
		"@vitest/browser": "^3.2.4",
		"axe-playwright": "^2.2.2",
		"bits-ui": "^2.11.5",
		"clsx": "^2.1.1",
		"eslint": "^9.37.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.4",
		"globals": "^16.4.0",
		"mode-watcher": "^1.1.0",
		"playwright": "^1.56.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"svelte": "^5.39.10",
		"svelte-check": "^4.3.2",
		"svelte-sonner": "^1.0.5",
		"tailwind-merge": "^3.3.1",
		"tailwind-variants": "^3.1.1",
		"tailwindcss": "^4.1.13",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.0",
		"vite": "^7.1.9",
		"vitest": "^3.2.4",
		"vitest-browser-svelte": "^1.1.0"
	},
	"dependencies": {
		"@aws-amplify/auth": "^6.16.0",
		"@storybook/addon-essentials": "^9.0.0-alpha.12",
		"@storybook/addon-svelte-csf": "^5.0.10",
		"@storybook/sveltekit": "^9.1.10",
		"amazon-cognito-identity-js": "^6.3.15",
		"aws-amplify": "^6.15.6"
	}
}
